<p id="notice"><%= notice %></p>
<h2><%= @team.name %></h2>
<ul>
<% @team.users.each do |user| %>
  <li><%= user.username %></li>
<% end %>
</ul>

<% if @team.users.count < 2 %>
<%= link_to 'Add User to Team', '#' %> |

<% end %>
<%= link_to 'Edit', edit_team_path(@team) %> |
<%= link_to 'Back', teams_path %>
